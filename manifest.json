{
  "manifest_version": 3,
  "name": "CALL",
  "version": "4.0",
  "description": "The Lite version Of Chrome App Launcher",

  "permissions": ["tabs", "storage", "sidePanel"],

  "host_permissions": ["<all_urls>"],

  "action": {
    "default_popup": "index.html",
    "default_title": "Open Launcher"
  },

  "side_panel": {
    "default_path": "index.html"
  },

  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}
